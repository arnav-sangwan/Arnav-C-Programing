#include <stdio.h>

int main() 
{
 // basic add,sub,multiply,divide      
   int num1=18,num2=3,result;
   result=num1+num2;
   printf("\n%d",result);
    result=num1-num2;
    printf("\n%d",result);
     result=num1*num2;
     printf("\n%d",result);
      result=num1/num2;
      printf("\n%d",result);
    return 0;
}
